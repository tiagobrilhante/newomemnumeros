{
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "types": [
      "@types/node",
      "@nuxt/types",
      "@prisma/client"
    ],
    "typeRoots": [
      "./node_modules/@types"
    ],
    "moduleResolution": "bundler",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true

  }
}
